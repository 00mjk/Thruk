[% PROCESS _header.tt css="thruk/plugins/business_process/bp.css" js=["thruk/plugins/business_process/bp.js", "thruk/plugins/business_process/dagre-0.2.0/dagre.min.js" ] %]
[% PROCESS _message.tt %]

[% UNLESS minimal %]
<table border="0" width="100%" cellspacing="0" cellpadding="0">
  <tr>
    <td align="left" valign="top" width="33%">
      [% PROCESS _infobox.tt %]
      [% IF subtitle != 'Business Process Overview' %]
      <table border="1" cellpadding="0" cellspacing="0" class='linkBox'>
        <tr>
          <td class='linkBox'>
            <a href='bp.cgi'>List All Business Processes</a><br>
          </td>
        </tr>
      </table>
      [% END %]
    </td>
    <td align="center" valign="top" width="33%">
      <br><br>
      <div align="center" class='dataTitle'>
        <span id="subtitle">[% subtitle %]</span>
        [% IF editmode %]<br><span style="color: #EB6900">(Edit-Mode)</span>[% END %]
      </div>
    </td>
    <td align="right" valign="bottom" width="33%">
      [% PROCESS _help.tt topic = "business_process" %]
    </td>
  </tr>
</table>
[% END %]

[% content %]

<script type="text/javascript">
<!--
init_bp_buttons();
-->
</script>